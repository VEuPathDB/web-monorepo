{
  "name": "wdk-client",
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "build": "webpack --hide-modules",
    "check-ts": "tsc --version && tsc --noEmit",
    "watch-ts": "tsc --noEmit --watch -p .",
    "examples": "cd client-examples && webpack-dev-server --content-base public/"
  },
  "private": true,
  "files": [
    "src/*"
  ],
  "main": "./src/Core",
  "dependencies": {
    "@types/datatables.net": "^1.10.5",
    "@types/flot": "^0.0.31",
    "@types/history": "^4.7.4",
    "@types/jqueryui": "^1.11.35",
    "@types/json-stable-stringify": "^1.0.31",
    "@types/lodash": "^4.14.104",
    "@types/natural-sort": "^0.0.20",
    "@types/node": "^11.12.2",
    "@types/prop-types": "^15.7.3",
    "@types/qtip2": "^2.2.29",
    "@types/react": "^16.9.18",
    "@types/react-autocomplete": "^1.8.4",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-select": "^3.0.4",
    "classnames": "^2.2.0",
    "history": "^4.10.1",
    "json-stable-stringify": "^1.0.0",
    "localforage": "^1.5.0",
    "lodash": "^4.17.5",
    "mansion-ui": "^2.5.2",
    "moment": "^2.21.0",
    "natural-sort": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-autocomplete": "^1.8.1",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.4",
    "react-transition-group": "^2.2.1",
    "redux": "^4.0.0",
    "redux-observable": "^1.2.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.2",
    "spin.js": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "babel-eslint": "^8.0.1",
    "eslint": "^5.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "ts-node": "^7.0.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleNameMapper": {
      "^wdk-client(.*)$": "<rootDir>/src$1"
    },
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "optionalDependencies": {
    "babel-jest": "^23.1.2",
    "jest": "^23.4.2",
    "ts-jest": "^23.1.2"
  }
}
