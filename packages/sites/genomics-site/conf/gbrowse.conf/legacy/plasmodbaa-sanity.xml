    <test name="match:BLASTP" min="1" max="50">
    </test>

    <test name="domain:ExportPred:subfeatures" min="1" max="50">
      <param name="parent_id" parentFeatureTest="domain:ExportPred"/>
    </test>

    <test name="protein:seq" min="1" max="50">
      <param name="id" value="PF11_0344"/>
    </test>


