{
  "name": "@veupathdb/genomics-site",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "veupathdb-react-scripts run-site-dev-server --config webpack.config.local.mjs",
    "clean": "rm -rf dist && mkdir dist",
    "copy:webapp": "cp -r webapp/* dist",
    "copy:images": "cp -r ../../../node_modules/@veupathdb/web-common/images dist",
    "compile:check": "tsc --noEmit",
    "bundle:dev": "npm-run-all clean copy:webapp copy:images && BROWSERSLIST_ENV=modern webpack --mode=development",
    "bundle:npm": "npm-run-all clean copy:webapp copy:images && BROWSERSLIST_ENV=modern webpack --mode=production && BROWSERSLIST_ENV=legacy webpack --mode=production"
  },
  "browserslist": [
    "extends @veupathdb/browserslist-config"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.14.5",
    "@emotion/react": "^11.10.0",
    "@emotion/serialize": "^1.0.2",
    "@emotion/styled": "^11.10.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@types/cytoscape": "^3.14.7",
    "@types/jqueryui": "^1.12.16",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "3",
    "@veupathdb/base-webpack-config": "workspace:^",
    "@veupathdb/blast-summary-view": "workspace:^",
    "@veupathdb/browserslist-config": "workspace:^",
    "@veupathdb/components": "workspace:^",
    "@veupathdb/coreui": "workspace:^",
    "@veupathdb/eda": "workspace:^",
    "@veupathdb/eslint-config": "workspace:^",
    "@veupathdb/http-utils": "workspace:^",
    "@veupathdb/multi-blast": "workspace:^",
    "@veupathdb/preferred-organisms": "workspace:^",
    "@veupathdb/react-scripts": "workspace:^",
    "@veupathdb/site-babel-config": "workspace:^",
    "@veupathdb/site-tsconfig": "workspace:^",
    "@veupathdb/site-webpack-config": "workspace:^",
    "@veupathdb/study-data-access": "workspace:^",
    "@veupathdb/user-datasets": "workspace:^",
