    "core-js": "^3.25.3",
    "css-loader": "^2.1.1",
    "cytoscape": "^3.15.2",
    "cytoscape-dagre": "^1.3.0",
    "cytoscape-panzoom": "^2.5.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.5.2",
    "ify-loader": "^1.1.0",
    "jquery": "1.9.1",
    "js-cookie": "^2.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "notistack": "^1.0.10",
    "npm-run-all": "^4.1.5",
